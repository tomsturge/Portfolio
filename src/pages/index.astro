---
import { DefaultLayout } from "@/ui/layouts";
import { HomePage } from "@/ui/pages";

import { Seo } from "@/ui/components";

const canonicalURL = new URL(Astro.url.pathname, Astro.site);
---

<DefaultLayout>
  <Fragment slot="seo">
    <Seo url={canonicalURL} />
  </Fragment>

  <HomePage />
</DefaultLayout>
